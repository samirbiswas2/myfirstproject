	<sub-flow name="insertEmployeeSub_Flow" doc:id="7fb8e77d-f0e9-46e3-873a-3e14acdc9368" >
		<db:insert doc:name="Insert" doc:id="ad4d804f-4e66-4138-b508-d23174b3329e" config-ref="Database_Config">
			<db:sql ><![CDATA[insert into employee (empid, first_name, last_name, email_id, mobile, address, domain ) values (:empid, :firstname, :lastname, :email, :mobile, :address, :domain)]]></db:sql>
			<db:input-parameters ><![CDATA[#[{
	empid : payload.empid,
	firstname : payload.firstname,
	lastname : payload.lastname,
	email : payload.email,
	mobile : payload.mobile,
	address : payload.address,
	domain : payload.domain
	
}]]]></db:input-parameters>
		</db:insert>
	</sub-flow>
