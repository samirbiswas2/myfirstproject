http://www.mulesoft.org/schema/mule/db http://www.mulesoft.org/schema/mule/db/current/mule-db.xsd">
	<sub-flow name="getEmployeeSub_Flow" doc:id="1cfb5c2c-5976-44ce-9df2-5eb2c8e06ba5" >
		<db:select doc:name="Select" doc:id="320c349a-114f-4625-8128-1681da265c50" config-ref="Database_Config">
			<db:sql ><![CDATA[select * from employee]]></db:sql>
		</db:select>
	</sub-flow>
	<sub-flow name="getEmployeeByEmpIdSub_Flow" doc:id="5cfd935f-6eae-4baf-b99c-87fb067b34b3" >
		<db:select doc:name="Select" doc:id="14abb178-5476-4175-99bd-ee2fe112da79" config-ref="Database_Config">
			<db:sql ><![CDATA[select * from employee where empid = :empid]]></db:sql>
			<db:input-parameters ><![CDATA[#[{
	empid : vars.empid
}]]]></db:input-parameters>
		</db:select>
	</sub-flow>
	<sub-flow name="deleteEmployeeByEmpIdSub_Flow" doc:id="69509e8a-bbbc-4e16-909d-c9f07943ff0a" >
		<db:delete doc:name="Delete" doc:id="1ae60347-b0bc-4be9-8079-c958238c0f2e" config-ref="Database_Config">
